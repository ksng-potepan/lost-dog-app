<!DOCTYPE html>
<html>
  <%= render 'layouts/head' %>
  <body style="background-color:#BFCCB5">
    <% if flash[:notice]%>
      <div class="notice"><%= flash[:notice]%></div>
    <% end %>

    <% if flash[:alert]%>
      <div class="alert"><%= flash[:alert]%></div>
    <% end %>
    <div class='main-wrapper'>
      <%= render 'layouts/header' %>
      <%= yield %>
    </div>
    <%= javascript_importmap_tags %>
  </body>
</html>
